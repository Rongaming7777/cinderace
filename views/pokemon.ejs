<script>
							function quanmultiply_<%= pokemon.id %>() {
									 var quant_<%= pokemon.id %> = document.getElementById('quan_<%= pokemon.id %>').value;
										var price_<%= pokemon.id %> = document.getElementById('price_<%= pokemon.id %>').value;
										var min_<%= pokemon.id %> = <%= pokemon.min %>
										var result1_<%= pokemon.id %> = price_<%= pokemon.id %> / min_<%= pokemon.id %>;
										var result_<%= pokemon.id %> = result1_<%= pokemon.id %> * quant_<%= pokemon.id %> 
										document.getElementById('totalprice_<%= pokemon.id %>').value = result_<%= pokemon.id %>;

											}
							</script>
				
